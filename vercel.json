{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },

    { "src": "^/$", "dest": "/index.html" },

    { "src": "^/resume\\.pdf$", "dest": "/public/resume.pdf" },
    { "src": "^/Oldresume\\.pdf$", "dest": "/public/Oldresume.pdf" },
    { "src": "^/og-card\\.png$", "dest": "/public/og-card.png" },
    { "src": "^/robots\\.txt$", "dest": "/public/robots.txt" },
    { "src": "^/sitemap\\.xml$", "dest": "/public/sitemap.xml" },

    { "src": "^/assets/(.*)$", "dest": "/public/assets/$1" }
  ]
}
